   "source": [
    "def sort_priority(values, group):\n",
    "    def helper(x):\n",
    "        if x in group:\n",
    "            return (0, x)\n",
    "        return (1, x)\n",
    "    values.sort(key=helper)\n",
    "    \n",
    "numbers = [8, 3, 2, 4,1,5]\n",
    "group = { 2, 3, 5, 6}\n",
    "\n",
    "sort_priority(numbers, group)\n",
    "print (numbers)"
   ]
